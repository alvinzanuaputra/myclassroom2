{
  "name": "myclassroom-backend",
  "version": "1.0.0",
  "description": "Backend API untuk MyClassroom - Rubrik Penilaian Guru",
  "main": "src/server.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "build": "npx prisma generate",
    "vercel-build": "npx prisma generate && npx prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "db:push": "prisma db push",
    "db:seed": "node prisma/seed.js",
    "db:deploy": "prisma migrate deploy"
  },
  "keywords": ["classroom", "assessment", "education"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
