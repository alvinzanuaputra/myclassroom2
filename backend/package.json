{
  "name": "myclassroom-backend",
  "version": "1.0.0",
  "description": "Backend API untuk MyClassroom - Rubrik Penilaian Guru",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "npx prisma generate",
    "vercel-build": "npx prisma generate && npx prisma migrate deploy",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:push": "npx prisma db push",
    "db:seed": "node prisma/seed.js",
    "db:reset": "npx prisma migrate reset"
  },
  "keywords": [
    "classroom",
    "assessment",
    "education"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
